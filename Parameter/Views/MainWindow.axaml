<Window xmlns="https://github.com/avaloniaui"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:vm="using:Parameter.ViewModels"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:views="clr-namespace:Parameter.Views"
		xmlns:i="using:Avalonia.Xaml.Interactivity"
		xmlns:bh="using:Parameter.Behaviors"
		mc:Ignorable="d"
		d:DesignWidth="500" d:DesignHeight="550"
		Width="500" Height="550"
		MinWidth="450"
		MinHeight="300"
		x:Class="Parameter.Views.MainWindow"
		Focusable="True"
		Background="{DynamicResource WindowBackgroundBrush}"
		Icon="/Assets/Images/logo.ico"
		Title="Parameter">

	<Design.DataContext>
		<vm:MainViewModel />
	</Design.DataContext>

	<i:Interaction.Behaviors>
		<bh:WindowStateBehavior />
	</i:Interaction.Behaviors>

	<views:MainView />

</Window>
